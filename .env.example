# Discord Bot Token
DISCORD_TOKEN=your_discord_bot_token_here

# Discord Guild ID (Server ID)
GUILD_ID=your_guild_id_here

# Google Sheets Configuration
GOOGLE_SHEET_ID=your_google_sheet_id_here
GOOGLE_SHEET_RANGE=Master_CSV!A:Z

# Google API Key (from Google Cloud Console) - for read-only access
GOOGLE_API_KEY=your_google_api_key_here

# Google Service Account (for write access) - REQUIRED for /move and /hold commands
# These commands write to Master_CSV sheet column E (Action)
# Download JSON file from Google Cloud Console and paste the path here
GOOGLE_SERVICE_ACCOUNT_PATH=./credentials.json

# Season Number (optional, default: 156)
SEASON_NUMBER=156
